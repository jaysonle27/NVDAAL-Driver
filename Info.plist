<dict>
    <key>CFBundleDevelopmentRegion</key>
    <string>English</string>
    <key>CFBundleExecutable</key>
    <string>NVDAAL</string>
    <key>CFBundleIdentifier</key>
    <string>com.nvdaal.compute</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>NVDAAL</string>
    <key>CFBundlePackageType</key>
    <string>KEXT</string>
    <key>CFBundleShortVersionString</key>
    <string>0.1.0</string>
    <key>CFBundleVersion</key>
    <string>0.1.0</string>
    <key>IOKitPersonalities</key>
    <dict>
        <!-- RTX 4090 (AD102) - Primary target -->
        <key>NVDAAL-RTX4090</key>
        <dict>
            <key>CFBundleIdentifier</key>
            <string>com.nvdaal.compute</string>
            <key>IOClass</key>
            <string>NVDAAL</string>
            <key>IOProviderClass</key>
            <string>IOPCIDevice</string>
            <!-- PCI matching: Device ID 0x2684 + Vendor ID 0x10DE -->
            <key>IOPCIPrimaryMatch</key>
            <string>0x268410DE 0x268510DE</string>
            <!-- Also match by name for broader compatibility -->
            <key>IONameMatch</key>
            <array>
                <string>display</string>
                <string>pci10de,2684</string>
                <string>pci10de,2685</string>
            </array>
            <!-- High probe score to beat IONDRVFramebuffer (20000) -->
            <key>IOProbeScore</key>
            <integer>25000</integer>
            <!-- Thunderbolt/eGPU support -->
            <key>IOPCITunnelCompatible</key>
            <true/>
            <key>IOPCIPrimaryMatchIfParentIsPCIBridge</key>
            <true/>
        </dict>
        <!-- RTX 4080/4080 SUPER -->
        <key>NVDAAL-RTX4080</key>
